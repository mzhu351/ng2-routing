{
	"complierOptions": {
		"target": "es5",
		"module": "commonjs",
		"moduleResolution": "node",
		"sourceMap": true,
		"emitDecoratorMetaData": true,
		"experimentalDecorators": true,
		"allowJs": true,
		"removeComments": false,
		"noImplicitAny": false,
		"outDir": "dist"
	},
	"exclude": [
		"node_modules"
	]
}
